<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ifsp.covid.controller.BulletinController">
   <children>
      <TextField fx:id="inpCity" layoutX="226.0" layoutY="83.0" promptText="City" />
      <TextField fx:id="inpIcuRatio" layoutX="226.0" layoutY="181.0" promptText="IcuRatio" />
      <TextField fx:id="inpDeaths" layoutX="226.0" layoutY="148.0" promptText="Deaths" />
      <TextField fx:id="inpInfected" layoutX="226.0" layoutY="116.0" promptText="Infected" />
      <DatePicker fx:id="inpDate" layoutX="214.0" layoutY="220.0" />
      <ComboBox fx:id="cbState" layoutX="225.0" layoutY="49.0" prefWidth="150.0" />
      <Button fx:id="btnExec" layoutX="518.0" layoutY="351.0" mnemonicParsing="false" text="Salvar" onAction="#execute" />
      <Button fx:id="btnCancelar" layoutX="450.0" layoutY="351.0" mnemonicParsing="false" text="Cancelar" onAction="#cancelar"/>
   </children>
</AnchorPane>
